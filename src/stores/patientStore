import {defineStore} from 'pinia'
import type { Patient } from '@/Models/Patient'

export const usePatients = defineStore('patients' {
    state: () => ({
        patients:[] as Patient[],
    }),

    getters: {
        getPatients(state) {
            return state.patients
        },
    },

    actions: {
        addPatients(patients: Patient[]){
           this.patients = [...patients]
        } 
    }
        
    }),

    

